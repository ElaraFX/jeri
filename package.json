{
  "name": "jeriview",
  "version": "0.3.6",
  "license": "SEE LICENSE IN FILE LICENSE",
  "private": true,
  "dependencies": {
    "@types/react": "15.6.1",
    "@types/react-dom": "15.5.6",
    "common-prefix": "1.1.0",
    "normalize-wheel": "1.0.1",
    "styled-components": "2.2.2"
  },
  "devDependencies": {
    "tslint": "5.8.0",
    "tslint-react": "3.2.0",
    "serve": "6.3.1",
    "typedoc": "0.9.0",
    "typescript": "2.5.3",
    "webpack": "3.8.1",
    "file-loader": "1.1.9",
    "json-loader": "0.5.7",
    "worker-loader": "1.1.0",
    "source-map-loader": "0.2.3"
  },
  "peerDependencies": {
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "scripts": {
    "build": "npm run build-npm && npm run build-web",
    "build-npm": "tsc && cp -r ./src/exr-wrap build_npm && cp -r ./src/utils/exr-parser.worker.js build_npm/utils",
    "build-web": "webpack",
    "clean": "rm -rf build build_web/jeriview* build_web/*.worker.js build_web/*.mem build_web/*.worker.js.map",
    "docs": "typedoc --out documentation src",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "examples": "serve -s build_web"
  },
  "main": "build_npm/jeriview.js",
  "types": "build_npm/jeriview.d.ts"
}
